{
	"name": "foxbin2prg-syntax-highlighting",
	"displayName": "FoxPro.sc2 Syntax Highlighting",
	"description": "Syntax Highlighting and Snippets for FoxPro SC2 files",
	"scripts": {
		"package": "vsce package"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/omarqui/vscode-foxpro-sc2.git"
	},
	"icon": "fox.png",
	"version": "1.4.12",
	"author": "Tom Bellin",
	"publisher": "omarqui",
	"engines": {
		"vscode": "^1.8.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"contributes": {
		"configurationDefaults": {
			"[foxbin2prg]": {
				"files.encoding": "windows1252"
			}
		},
		"languages": [
			{
				"id": "foxbin2prg",
				"aliases": [
					"FoxBin2Prg",
					"sc2"
				],
				"extensions": [
					".sc2",
					".SC2",
					".vc2",
					".VC2"
				],
				"configuration": "./foxpro-configuration.json"
			}
		],
		"snippets": [
			{
				"language": "foxbin2prg",
				"path": "./snippets/foxpro_snippets.json"
			}
		],
		"grammars": [
			{
				"language": "foxbin2prg",
				"scopeName": "source.foxbin2prg",
				"path": "./syntaxes/foxpro.plist"
			}
		]
	},
	"devDependencies": {
		"@vscode/vsce": "^3.7.1"
	}
}
